<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HeadEditTextView">
        <!--背景色-->
        <attr name="headEditTextBackgroundColor" format="color" />
        <!--是否支持渐变-->
        <attr name="headEditTextSupportGradient" format="boolean" />
        <!--渐变起始色-->
        <attr name="headEditTextGradientFrom" format="color" />
        <!--渐变结束色-->
        <attr name="headEditTextGradientTo" format="color" />
        <!--圆角弧度-->
        <attr name="headEditTextRadians" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headEditTextRadianLeftTop" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headEditTextRadianRightTop" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headEditTextRadianLeftBottom" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headEditTextRadianRightBottom" format="dimension" />
        <!--边框颜色-->
        <attr name="headEditTextStrokeColor" format="color" />
        <!--设置边框宽度-->
        <attr name="headEditTextStrokeWidth" format="dimension" />
        <!--设置边框为虚线的长度-->
        <attr name="headEditTextStrokeDashWidth" format="float" />
        <!--设置虚线之间的像素间距-->
        <attr name="headEditTextStrokeDashGap" format="float" />
        <!--设置开启右侧的清空内容事件 默认关闭-->
        <attr name="headEditTextClear" format="boolean" />
        <!--设置开启右侧的清空内容按钮是否根据焦点来控制显示还是不显示，默认关闭这个开关-->
        <attr name="headEditTextClearIcon" format="boolean" />
        <!--控件类型-->
        <attr name="headEditTextType">
            <enum name="editText" value="0" />
            <enum name="textView" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="HeadButton">
        <!--背景色 默认-->
        <attr name="headButtonNormalBackgroundColor" format="color" />
        <!--是否支持渐变 默认-->
        <attr name="headButtonNormalSupportGradient" format="boolean" />
        <!--渐变起始色 默认-->
        <attr name="headButtonNormalGradientFrom" format="color" />
        <!--渐变结束色 默认-->
        <attr name="headButtonNormalGradientTo" format="color" />
        <!--圆角弧度 默认-->
        <attr name="headButtonNormalRadians" format="dimension" />
        <!--单独设置每个角 默认-->
        <attr name="headButtonNormalRadianLeftTop" format="dimension" />
        <!--单独设置每个角 默认-->
        <attr name="headButtonNormalRadianRightTop" format="dimension" />
        <!--单独设置每个角 默认-->
        <attr name="headButtonNormalRadianLeftBottom" format="dimension" />
        <!--单独设置每个角 默认-->
        <attr name="headButtonNormalRadianRightBottom" format="dimension" />
        <!--边框颜色 默认-->
        <attr name="headButtonNormalStrokeColor" format="color" />
        <!--设置边框宽度 默认-->
        <attr name="headButtonNormalStrokeWidth" format="dimension" />
        <!--设置边框为虚线的长度 默认-->
        <attr name="headButtonNormalStrokeDashWidth" format="float" />
        <!--设置虚线之间的像素间距 默认-->
        <attr name="headButtonNormalStrokeDashGap" format="float" />


        <!--背景色 按下-->
        <attr name="headButtonPressedBackgroundColor" format="color" />
        <!--是否支持渐变 按下-->
        <attr name="headButtonPressedSupportGradient" format="boolean" />
        <!--渐变起始色 按下-->
        <attr name="headButtonPressedGradientFrom" format="color" />
        <!--渐变结束色 按下-->
        <attr name="headButtonPressedGradientTo" format="color" />
        <!--圆角弧度 按下-->
        <attr name="headButtonPressedRadians" format="dimension" />
        <!--单独设置每个角 按下-->
        <attr name="headButtonPressedRadianLeftTop" format="dimension" />
        <!--单独设置每个角 按下-->
        <attr name="headButtonPressedRadianRightTop" format="dimension" />
        <!--单独设置每个角 按下-->
        <attr name="headButtonPressedRadianLeftBottom" format="dimension" />
        <!--单独设置每个角 按下-->
        <attr name="headButtonPressedRadianRightBottom" format="dimension" />
        <!--边框颜色 按下-->
        <attr name="headButtonPressedStrokeColor" format="color" />
        <!--设置边框宽度 按下-->
        <attr name="headButtonPressedStrokeWidth" format="dimension" />
        <!--设置边框为虚线的长度 按下-->
        <attr name="headButtonPressedStrokeDashWidth" format="float" />
        <!--设置虚线之间的像素间距 按下-->
        <attr name="headButtonPressedStrokeDashGap" format="float" />


        <!--背景色 禁止-->
        <attr name="headButtonEnabledBackgroundColor" format="color" />
        <!--是否支持渐变 禁止-->
        <attr name="headButtonEnabledSupportGradient" format="boolean" />
        <!--渐变起始色 禁止-->
        <attr name="headButtonEnabledGradientFrom" format="color" />
        <!--渐变结束色 禁止-->
        <attr name="headButtonEnabledGradientTo" format="color" />
        <!--圆角弧度 禁止-->
        <attr name="headButtonEnabledRadians" format="dimension" />
        <!--单独设置每个角 禁止-->
        <attr name="headButtonEnabledRadianLeftTop" format="dimension" />
        <!--单独设置每个角 禁止-->
        <attr name="headButtonEnabledRadianRightTop" format="dimension" />
        <!--单独设置每个角 禁止-->
        <attr name="headButtonEnabledRadianLeftBottom" format="dimension" />
        <!--单独设置每个角 禁止-->
        <attr name="headButtonEnabledRadianRightBottom" format="dimension" />
        <!--边框颜色 禁止-->
        <attr name="headButtonEnabledStrokeColor" format="color" />
        <!--设置边框宽度 禁止-->
        <attr name="headButtonEnabledStrokeWidth" format="dimension" />
        <!--设置边框为虚线的长度 禁止-->
        <attr name="headButtonEnabledStrokeDashWidth" format="float" />
        <!--设置虚线之间的像素间距 禁止-->
        <attr name="headButtonEnabledStrokeDashGap" format="float" />
        <!--设置按钮是矩形还是椭圆形 默认矩形-->
        <attr name="headButtonShape">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
        </attr>


    </declare-styleable>


    <declare-styleable name="HeadSpinner">
        <attr name="headSpinnerWidth" format="dimension">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="headSpinnerHeight" format="dimension">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="headSpinnerDropDownGravity">
            <!--左对齐-->
            <enum name="LEFT" value="0" />
            <!--右对齐-->
            <enum name="RIGHT" value="1" />
            <!--居中对齐-->
            <enum name="CENTER" value="2" />
        </attr>
        <!--相对锚点偏移值，正值暗示向右偏移-->
        <attr name="headSpinnerHorizontalOffset" format="dimension" />
        <!--相对锚点偏移值，正值暗示向下偏移-->
        <attr name="headSpinnerVerticalOffset" format="dimension" />

        <!--背景色-->
        <attr name="headSpinnerBackgroundColor" format="color" />
        <!--是否支持渐变-->
        <attr name="headSpinnerSupportGradient" format="boolean" />
        <!--渐变起始色-->
        <attr name="headSpinnerGradientFrom" format="color" />
        <!--渐变结束色-->
        <attr name="headSpinnerGradientTo" format="color" />
        <!--圆角弧度-->
        <attr name="headSpinnerRadians" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headSpinnerRadianLeftTop" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headSpinnerRadianRightTop" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headSpinnerRadianLeftBottom" format="dimension" />
        <!--单独设置每个角-->
        <attr name="headSpinnerRadianRightBottom" format="dimension" />
        <!--边框颜色-->
        <attr name="headSpinnerStrokeColor" format="color" />
        <!--设置边框宽度-->
        <attr name="headSpinnerStrokeWidth" format="dimension" />
        <!--设置边框为虚线的长度-->
        <attr name="headSpinnerStrokeDashWidth" format="float" />
        <!--设置虚线之间的像素间距-->
        <attr name="headSpinnerStrokeDashGap" format="float" />
        <!--设置Spinner的文本颜色-->
        <attr name="headSpinnerTextColor" format="color" />
        <!--设置Spinner的文本Size-->
        <attr name="headSpinnerTextSize" format="dimension" />
        <!--arrow是否显示-->
        <attr name="headSpinnerArrowVisibility">
            <enum name="visible" value="0" />
            <enum name="gone" value="1" />
        </attr>
        <!--arrow颜色-->
        <attr name="headSpinnerArrowColor" format="color" />

    </declare-styleable>
    <declare-styleable name="HeadTitleBar">
        <!--标题布局-->
        <attr name="headTitleBarCustomView" format="reference"/>
        <!--背景色-->
        <attr name="headTitleBarBackgroundColor" format="color" />
        <!--是否支持渐变-->
        <attr name="headTitleBarSupportGradient" format="boolean" />
        <!--渐变起始色-->
        <attr name="headTitleBarGradientFrom" format="color" />
        <!--渐变结束色-->
        <attr name="headTitleBarGradientTo" format="color" />

        <!--是否开启透明状态栏 默认关闭-->
        <attr name="headTitleBarTransparent" format="boolean" />
        <!--是否填充状态栏 默认不填充-->
        <attr name="headTitleBarFill" format="boolean" />
        <!--状态栏的填充色如果有参数则填充，如果没有则默认和标题栏同色-->
        <attr name="headTitleBarFillColor" format="color" />
        <!--填充透明度-->
        <attr name="headTitleBarFillColorAlpha" format="integer" />
        <!--状态栏主题-->
        <attr name="headTitleBarTheme" >
            <enum name="light" value="0" />
            <enum name="dark" value="1" />
        </attr>

        <!--标题栏风格-->
        <attr name="headTitleStyle">
            <enum name="general" value="0" />
            <enum name="search" value="1" />
            <enum name="custom" value="2" />
        </attr>
        <!--内置标题模版左侧文本-->
        <attr name="headTitleBuiltInLeftText" format="string"/>
        <!--内置标题模版左侧图标-->
        <attr name="headTitleBuiltInLeftIcon" format="reference"/>
        <!--内置标题模版左侧文字大小-->
        <attr name="headTitleBuiltInLeftTextSize" format="dimension"/>
        <!--内置标题模版左侧文字颜色-->
        <attr name="headTitleBuiltInLeftTextColor" format="color"/>

        <!--内置标题模版右侧文本-->
        <attr name="headTitleBuiltInRightText" format="string"/>
        <!--内置标题模版右侧图标-->
        <attr name="headTitleBuiltInRightIcon" format="reference"/>
        <!--内置标题模版右侧文字大小-->
        <attr name="headTitleBuiltInRightTextSize" format="dimension"/>
        <!--内置标题模版右侧文字颜色-->
        <attr name="headTitleBuiltInRightTextColor" format="color"/>

        <!--通用标题模版中间主标题文本-->
        <attr name="headTitleGeneralCenterMainText" format="string"/>
        <!--通用标题模版中间主标题文字大小-->
        <attr name="headTitleGeneralCenterMainTextSize" format="dimension"/>
        <!--通用标题模版中间主标题文字颜色-->
        <attr name="headTitleGeneralCenterMainTextColor" format="color"/>
        <!--通用标题模版中间主标题是否开启跑马灯效果-->
        <attr name="headTitleGeneralCenterMainMarquee" format="boolean"/>

        <!--通用标题模版中间次标题文本-->
        <attr name="headTitleGeneralCenterSubText" format="string"/>
        <!--通用标题模版中间次标题文字大小-->
        <attr name="headTitleGeneralCenterSubTextSize" format="dimension"/>
        <!--通用标题模版中间次标题文字颜色-->
        <attr name="headTitleGeneralCenterSubTextColor" format="color"/>
        <!--通用标题模版中间次标题是否开启跑马灯效果-->
        <attr name="headTitleGeneralCenterSubMarquee" format="boolean"/>


        <!--搜索标题栏的是否弹出软键盘-->
        <attr name="headTitleSearchSoftInputKeyBoard" format="boolean" />
        <!--搜索标题栏的输入框左侧图标-->
        <attr name="headTitleSearchLeftIcon" format="reference" />
        <!--搜索标题栏的输入框文本-->
        <attr name="headTitleSearchText" format="string"/>
        <!--搜索标题栏的输入框提示性文本-->
        <attr name="headTitleSearchHint" format="string" />
        <!--搜索标题栏的输入框提示性文本-->
        <attr name="headTitleSearchHintColor" format="color" />

        <!--搜索标题栏的输入框文本大小-->
        <attr name="headTitleSearchTextSize" format="dimension" />
        <!--搜索标题栏的输入框文本颜色-->
        <attr name="headTitleSearchTextColor" format="color" />

        <!--搜索标题栏的输入框背景色-->
        <attr name="headTitleSearchBackgroundColor" format="color" />
        <!--搜索标题栏的输入框是否支持渐变-->
        <attr name="headTitleSearchSupportGradient" format="boolean" />
        <!--搜索标题栏的输入框渐变起始色-->
        <attr name="headTitleSearchGradientFrom" format="color" />
        <!--搜索标题栏的输入框渐变结束色-->
        <attr name="headTitleSearchGradientTo" format="color" />
        <!--搜索标题栏的输入框圆角弧度-->
        <attr name="headTitleSearchRadians" format="dimension" />
        <!--搜索标题栏的输入框单独设置每个角-->
        <attr name="headTitleSearchRadianLeftTop" format="dimension" />
        <!--搜索标题栏的输入框单独设置每个角-->
        <attr name="headTitleSearchRadianRightTop" format="dimension" />
        <!--搜索标题栏的输入框单独设置每个角-->
        <attr name="headTitleSearchRadianLeftBottom" format="dimension" />
        <!--搜索标题栏的输入框单独设置每个角-->
        <attr name="headTitleSearchRadianRightBottom" format="dimension" />
        <!--搜索标题栏的输入框边框颜色-->
        <attr name="headTitleSearchStrokeColor" format="color" />
        <!--搜索标题栏的输入框设置边框宽度-->
        <attr name="headTitleSearchStrokeWidth" format="dimension" />
        <!--搜索标题栏的输入框设置边框为虚线的长度-->
        <attr name="headTitleSearchStrokeDashWidth" format="float" />
        <!--搜索标题栏的输入框设置虚线之间的像素间距-->
        <attr name="headTitleSearchStrokeDashGap" format="float" />

    </declare-styleable>

</resources>